<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
   <metadata minClientVersion="2.8.1">
       <id>Matcha.BackgroundService</id>
       <version>1.0.5464</version>
       <title>A plugin library to simplify Backgrounding in Xamarin.Forms.</title>
       <authors>Winston Gubantes</authors>
       <owners>Matcha</owners>
       <licenseUrl>https://github.com/winstongubantes/MatchaBackgroundService/blob/master/LICENSE</licenseUrl>
       <projectUrl>https://github.com/winstongubantes/MatchaBackgroundService</projectUrl>
       <iconUrl>https://github.com/winstongubantes/MatchaBackgroundService/blob/master/images/icon.png</iconUrl>
       <requireLicenseAcceptance>false</requireLicenseAcceptance>
       <description>
         Ever wonder how facebook and twitter process there background to fetch a new content? It is not hard but you have to deal with each platform functionality, It is not generic and simple.
       </description>
     <releaseNotes>
        See: https://github.com/winstongubantes/MatchaBackgroundService/blob/master/CHANGELOG.md
     </releaseNotes>
       <summary>A plugin library to simplify Backgrounding in Xamarin.Forms.</summary>
       <tags>xamarin, android, ios, backgroundservice</tags>
       <dependencies>
         <group targetFramework="netstandard2.0">
			<dependency id="System.Reactive" version="3.0.0" exclude="Build,Analyzers" />
         </group>
		 <group targetFramework="MonoAndroid10">
			<dependency id="System.Reactive" version="3.0.0" exclude="Build,Analyzers" />
         </group>
         <group targetFramework="Xamarin.iOS10">
			<dependency id="System.Reactive" version="3.0.0" exclude="Build,Analyzers" />
         </group>
		 <group targetFramework="uap">
			<dependency id="System.Reactive" version="3.0.0" exclude="Build,Analyzers" />
         </group>
       </dependencies>
   </metadata>
   <files> 
      <!--Core-->
     <file src="..\src\Matcha.BackgroundService\bin\Release\netstandard2.0\Matcha.BackgroundService.*" target="lib\netstandard2.0" />
  

     <!--Xamarin.Android-->
     <file src="..\src\Matcha.BackgroundService.Droid\bin\Release\Matcha.BackgroundService.*" target="lib\MonoAndroid10" />
     
 
     <!--Xamarin.iOS Unified-->
     <file src="..\src\Matcha.BackgroundService.iOS\bin\Release\Matcha.BackgroundService.*" target="lib\Xamarin.iOS10" />
	 
	 <!--UWP-->
      <file src="..\src\Matcha.BackgroundService.UWP\bin\x86\Release\Matcha.BackgroundService.*" target="lib\UAP10" />
   </files>
</package>
